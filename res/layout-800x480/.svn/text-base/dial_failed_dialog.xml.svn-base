<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent">
  
	<TextView android:id="@+id/dialFailedMessageTextView" android:text="@string/tgm_dial_failed_dialog_message" android:padding="5dp" android:layout_width="wrap_content" android:textSize="18dp" android:layout_height="wrap_content" android:layout_gravity="center"></TextView>
	
	<ImageView android:id="@+id/dialFailedMessageImageView" android:padding="10dp" android:src="@drawable/connect_fail" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_gravity="center"></ImageView>
	
	<Button android:text="@string/tgm_dial_failed_ok_btn_text" android:id="@+id/dialFailedMessageOKBtn" android:layout_gravity="center" android:layout_width="100dp"  android:layout_height="wrap_content"></Button>
</LinearLayout>
